{
  "name": "donatello-ui",
  "version": "0.0.0",
  "description": "ui for donatello mocking service",
  "main": "main.js",
  "scripts": {
    "release": "npm run build && npm run build:app",
    "build:app": "electron-packager . Donatello --icon=assets/donatello.icns",
    "build:client": "rm -rf dist && webpack --config=webpack.config.dev.js",
    "build:client:watch": "npm run build:client -- --watch",
    "serve": "electron .",
    "build:and:serve": "npm run build:client && npm run serve",
    "lint": "tslint -c tslint.json 'src/**/*.ts'"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@angular/compiler-cli": "4.3.1",
    "@types/electron": "^1.4.37",
    "@types/node": "^7.0.5",
    "css-loader": "^0.26.1",
    "electron": "^9.1.0",
    "extract-text-webpack-plugin": "2.0.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "node-sass": "^4.5.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.13.1",
    "ts-loader": "^2.0.0",
    "tslint": "^4.4.2",
    "tslint-loader": "^3.4.2",
    "typescript": "^2.1.6",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "@angular/animations": "4.3.1",
    "@angular/cdk": "^2.0.0-beta.8",
    "@angular/common": "4.3.1",
    "@angular/compiler": "4.3.1",
    "@angular/core": "4.3.1",
    "@angular/forms": "4.3.1",
    "@angular/http": "4.3.1",
    "@angular/material": "^2.0.0-beta.8",
    "@angular/platform-browser": "4.3.1",
    "@angular/platform-browser-dynamic": "4.3.1",
    "@angular/platform-server": "4.3.1",
    "@angular/router": "4.3.1",
    "core-js": "^2.4.1",
    "donatello-core": "0.0.1",
    "electron-packager": "^8.6.0",
    "jsoneditor": "^5.6.0",
    "reflect-metadata": "^0.1.9",
    "rxjs": "^5.3.1",
    "ts-helpers": "^1.1.2",
    "typescript": "2.3.0",
    "zone.js": "^0.8.10"
  }
